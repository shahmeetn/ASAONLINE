<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="VO.AmcVO" table="amc">
	
		<id name="amcid" type="int" column="AMCid">
			<generator class="increment" />
		</id>
		
		<property name="amctitle">
			<column name="AMCTitle" />
		</property>
		
		<property name="amcprice">
			<column name="AMCPrice" />
		</property>
		
		<property name="amcdesc">
			<column name="AMCDescription" />
		</property>
	</class>
</hibernate-mapping>